[run]
source = .
omit = 
    */venv/*
    */testing/integration/*
    */testing/__pycache__/*
    */__pycache__/*
    */node_modules/*
    */htmlcov/*
    */coverage/*
    */migrations/*
    */tests/*
    setup.py
    */site-packages/*
    run_unit_tests_with_coverage.py
    run_c1_tests_with_coverage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # Test files
    def test_
    class Test

[html]
directory = htmlcov
title = Unit Test Coverage Report